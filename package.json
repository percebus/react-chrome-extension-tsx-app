{
  "name": "react-chrome-extension-tsx-app",
  "version": "0.1.0",
  "description": "A web extension",
  "source": "manifest.json",
  "scripts": {
    "npm:install:global": "npm install --location=global yarn",
    "setup": "npm run npm:install:global",
    "standard": "standard --fix *.js",
    "ts-standard": "standard --fix src/**/*.tsx",
    "lint:js": "npm run standard",
    "lint:ts": "npm run ts-standard",
    "lint": "npm run lint:ts && npm run lint:ts",
    "preparcel": "npm run lint",
    "parcel": "parcel",
    "parcel:watch": "npm run parcel -- watch src/manifest.json --host localhost",
    "parcel:build": "npm run parcel -- build src/manifest.json",
    "dev": "npm run parcel:watch",
    "dist": "npm run parcel:build",
    "test": "exit 0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "author": "percebus",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "@parcel/config-default": "^2.6.2",
    "@parcel/config-webextension": "^2.6.2",
    "@parcel/transformer-sass": "^2.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-frame-component": "^4.1.3",
    "@types/webextension-polyfill": "^0.9.0",
    "parcel": "^2.6.2",
    "process": "^0.11.10",
    "standard": "latest",
    "ts-standard": "^11.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.21.4",
    "lodash": "^4.17.21",
    "react-dom": "^18.2.0",
    "react-lodash": "^0.1.2",
    "web-vitals": "^2.1.4"
  }
}
