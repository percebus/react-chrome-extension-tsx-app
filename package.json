{
  "name": "react-chrome-extension-tsx-app",
  "version": "0.1.0",
  "description": "A web extension",
  "source": "manifest.json",
  "scripts": {
    "npm:install:global": "npm install --location=global yarn",
    "setup": "npm run npm:install:global",
    "setup:ci": "npm run setup",
    "yarn:licenses:list:prod": "yarn licenses list --prod > dist/LICENSES.txt",
    "yarn:licenses:generate-disclaimer:prod": "yarn licenses generate-disclaimer --prod > dist/LICENSES.md",
    "yarn:licenses:prod": "npm run yarn:licenses:list:prod && npm run yarn:licenses:generate-disclaimer:prod",
    "LICENSES": "npm run yarn:licenses:prod",
    "standard": "standard --fix *.js",
    "ts-standard": "standard --fix src/**/*.ts src/**/*.tsx __tests__/**/*.tsx",
    "lint:js": "npm run standard",
    "lint:ts": "npm run ts-standard",
    "lint": "npm run lint:js && npm run lint:ts",
    "prejest:ci": "npm run lint",
    "jest:ci": "jest --ci",
    "test": "npm run jest:ci",
    "preparcel": "npm test",
    "parcel": "parcel",
    "parcel:watch": "npm run parcel -- watch src/manifest.json --host localhost",
    "parcel:build": "npm run parcel -- build src/manifest.json",
    "dev": "npm run parcel:watch",
    "predist": "rm -rf dist",
    "dist": "npm run parcel:build",
    "postdist": "npm run LICENSES"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "author": "percebus",
  "license": "SEE LICENSE IN LICENSE.md",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@parcel/babel-preset-env": "^2.6.2",
    "@parcel/config-default": "^2.6.2",
    "@parcel/config-webextension": "^2.6.2",
    "@parcel/transformer-sass": "^2.6.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-frame-component": "^4.1.3",
    "@types/webextension-polyfill": "^0.9.0",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-extended": "^3.0.1",
    "jest-junit": "^14.0.0",
    "parcel": "^2.6.2",
    "postcss": "^8.4.14",
    "process": "^0.11.10",
    "standard": "latest",
    "tailwindcss": "^3.1.6",
    "ts-standard": "latest"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react-dom": "^18.2.0",
    "react-lodash": "^0.1.2",
    "web-vitals": "^2.1.4"
  }
}
